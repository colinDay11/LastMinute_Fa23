void loop() {

  // Cabinet 2 & 8 //
  if (digitalRead(buttonpin) == HIGH) {
    myservo.write(90);
  } else {
    myservo.write(0);
  }

  // Concession 3 & 9//
  if (digitalRead(buttonPin) == HIGH) {
    myServo.write(140);
  } else {
    myServo.write(0);
  //Bottle 4 & 10//
  }
  if (digitalRead(ButtonPin) == HIGH) {
    MyServo.write(180);
  } else {
    MyServo.write(0);
  }
if (digitalRead(buttonPin5) == LOW) {
    digitalWrite(buttonPin11, LOW);
  } else {
    digitalWrite(buttonPin11, HIGH);  
    }
